{
  "crons": [
    {
      "path": "/api/cron/monitor-solana-mints",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/process-generation-jobs-v2",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/burn-cycle",
      "schedule": "*/1 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
